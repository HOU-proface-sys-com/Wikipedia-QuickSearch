{
    "manifest_version": 2,
    "name": "Wikipedia QuickSearch",
    "version": "0.0.0",
    "description": "in-page quick wikipedia search",
    "author": "Danel, danelpo",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "images/logo.png",
        "default_popup": "HomeButtonPopup/HomeButtonPopup.html"
    }
}